<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.answersystem.lr.answersystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".com.answersystem.lr.bbs.bbs" />
        <activity android:name=".com.answersystem.lr.log_in.log_in" />
        <activity android:name=".com.answersystem.lr.problem.problem" />
        <activity android:name=".com.answersystem.lr.home.home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".com.answersystem.lr.home.personalInformation"></activity>
    </application>

</manifest>